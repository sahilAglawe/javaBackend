package mypack;

public class firstclass {
  public int i =10, j = 20;

  public firstclass() {
	super();
  }

  public firstclass(int i, int j) {
	super();
	this.i = i;
	this.j = j;
  }
  
  public void show() {
	  System.out.println("first class show");
  }

  @Override
  public String toString() {
	return "firstclass [i=" + i + ", j=" + j + "]";
  }
  
  
}
