package aggregation;

public class Address {
	
	int doorNo;
	String area, city, state, country;
	
	

	public Address() {
		super();
	}

	


	public Address(int doorNo, String area, String city, String state, String country) {
		super();
		this.doorNo = doorNo;
		this.area = area;
		this.city = city;
		this.state = state;
		this.country = country;
	}




	@Override
	public String toString() {
		return "Address [doorNo=" + doorNo + ", area=" + area + ", city=" + city + ", state=" + state + ", country="
				+ country + "]";
	}




	public static void main(String[] args) {
		
		Address add1 = new Address(123, "ameerpeet", "hyd", "TL", "India");
		System.out.println(add1);
	}

}
