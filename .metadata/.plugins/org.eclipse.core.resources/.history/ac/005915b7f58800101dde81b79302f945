package kapilit;

public class mainClass {

	public static void main(String[] args) {
		
		firstInterface obj =(i, j)->{
			                    if(i.salary > j.salary)
			                    	return i;
			                    else
			                    	return j;
		                     };
		                     
		firstInterface obj1 =(x, y)->{
			                        if(x.salary+ y.salary > 150000)
			                        	return x;
			                        else
			                        	return y;
		};
		        
	  Employee e1 = new Employee(123, "sahil", 22, 40000);
	  Employee e2 = new Employee(222, "shikant", 32, 90000);
	  System.out.println(obj.compareTo(e1 , e2));
	  
	  Employee e3 = obj1.compareTo(e1 , e2);
	  System.out.println(e3);
	}

}
