package aggregation;

public class Employee extends Address {
	
	int empId;
	String empName;
	int age, salary;
    Address address;
	
	
	public Employee() {
		super();
	}


	

	public Employee(int doorNo, String area, String city, String state, String country, int empId, String empName,
			int age, int salary) {
		super(doorNo, area, city, state, country);
		this.empId = empId;
		this.empName = empName;
		this.age = age;
		this.salary = salary;
		this.address = address;
	}
	
	





	@Override
	public String toString() {
		return "Employee [empId=" + empId + ", empName=" + empName + ", age=" + age + ", salary=" + salary
				+ ", address=" + address + "]";
	}




	public static void main(String[] args) {
		
		 Address add1 = new  Address(123, "ammerpeet", "hyd", "TL", "India");
		 Address add2 = new  Address(222, "SadashivPeth", "pune", "Mh", "India");
		 Address add3 = new  Address(333, "mukharji Nagar", "Delhi", "DL", "India");
		 
		 Employee e1 = new Employee(44, "Sahil", 21, 200000, add1);
		 
		 System.out.println(e1);
		 
	}

}
